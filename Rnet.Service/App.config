<?xml version="1.0" encoding="utf-8"?>

<configuration>

    <configSections>
        <section name="rnet.service" type="Rnet.Service.RnetServiceConfigurationSection, Rnet.Service" />
    </configSections>

    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

    <rnet.service>
        <hosts>
            <add uri="http://*:12292/rnet/">
                <bus>
                    <connection uri="rnet.tcp://192.168.0.51:9001/" />
                </bus>
            </add>
        </hosts>
    </rnet.service>

    <system.diagnostics>
        <sharedListeners>
            <add name="Console" type="System.Diagnostics.ConsoleTraceListener" />
            <add name="File" type="System.Diagnostics.TextWriterTraceListener" initializeData="Rnet.Service.log" />
        </sharedListeners>
        <trace>
            <listeners>
                <add name="Console" />
                <add name="File" />
            </listeners>
        </trace>
        <sources>
            <source name="Rnet" switchValue="All">
                <listeners>
                    <add name="Console" />
                    <add name="File" />
                </listeners>
            </source>
        </sources>
    </system.diagnostics>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.1.0.0" newVersion="2.1.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Reactive.Core" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.5.0" newVersion="2.2.5.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Reactive.Interfaces" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.5.0" newVersion="2.2.5.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

</configuration>
<?xml version="1.0" encoding="utf-8" ?>

<configuration>

    <configSections>
        <section
            name="rnet.service"
            type="Rnet.Service.RnetServiceConfigurationSection, Rnet.Service"/>
    </configSections>

    <startup>
        <supportedRuntime
            version="v4.0"
            sku=".NETFramework,Version=v4.5" />
    </startup>

    <rnet.service>
        <hosts>
            <add
                uri="http://localhost:12292/rnet/">
                <bus>
                    <connection
                        uri="rnet.tcp://173.175.230.163:9999/" />
                </bus>
            </add>
        </hosts>
    </rnet.service>

    <system.diagnostics>
        <sharedListeners>
            <add
                name="Console"
                type="System.Diagnostics.ConsoleTraceListener">
                <filter
                    type="System.Diagnostics.EventTypeFilter"
                    initializeData="Information" />
            </add>
        </sharedListeners>
        <trace>
            <listeners>
                <add
                    name="Console" />
            </listeners>
        </trace>
        <sources>
            <source
                name="Rnet"
                switchValue="All" />
        </sources>
    </system.diagnostics>

</configuration>